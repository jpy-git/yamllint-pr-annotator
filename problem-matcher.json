{
    "problemMatcher": [
        {
            "owner": "yamllint-error",
            "severity": "error",
            "pattern": [
                {
                    "regexp": "^([^:]+):(\\d+):(\\d+):\\s+(\\[(warning|error)\\].*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "message": 4
                }
            ]
        }
    ]
}
